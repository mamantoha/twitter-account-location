{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "chrome": "readonly",
    "browser": "readonly",
    "cacheManager": "readonly",
    "window": "readonly"
  },
  "rules": {
    "no-unused-vars": ["warn", { "args": "none", "vars": "all" }],
    "no-console": "off"
  }
}
